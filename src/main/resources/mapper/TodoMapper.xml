<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.teamd.tt.TodoMapper">

    <select id="selRegion" resultType="String">
        SELECT region
        from region
    </select>
    
    <select id="selDetailRegion" resultType="String">
        SELECT region_detail
        FROM region_detail
        WHERE id_region = #{id_region}
    </select>

</mapper>